{
  "app": {
    "title": "Example App",
    "shortTitle": "new-app",
    "description": "",
    "baseHref": "/",
    "analytics": "",
    "firebaseVersion": "4.2.0",
    "database": "dev",
    "waitBeforeFetchingSameURLInRest": 60,
    "sentryUrl": "",
